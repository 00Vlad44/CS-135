;; Example 1
(define (divisors num counter divisor)
  (cond
    [(> divisor num) counter]
    [(= (modulo num divisor) 0) (add1 counter)]
    [else (even-nats num counter (add1 divisor))]))

(define (count-disviors n)
  (divisors n 0 1))
  